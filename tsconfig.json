{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": [
      "node",
      "@playwright/test"
    ],
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ]
}